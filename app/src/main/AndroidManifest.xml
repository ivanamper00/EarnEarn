        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.NỔHŨLUCKY7VIP" >
        <activity
            android:name="com.urnmani.vroung.presentation.activity.SplashActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.urnmani.vroung.presentation.activity.MainActivity"
            android:exported="false" />
    </application>

</manifest>